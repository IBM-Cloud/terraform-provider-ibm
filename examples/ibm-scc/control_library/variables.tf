variable "scc_control_library_name" {
  description = "The name of the control library."
  type        = string
  default     = "scc_demo_control_library"
}

variable "scc_control_library_description" {
  description = "The description of the control library."
  type        = string
  default     = "This control library was made for demo purposes"
}

variable "scc_control_library_version" {
  description = "The version of the control library"
  type        = string
  default     = "0.0.1"
}

